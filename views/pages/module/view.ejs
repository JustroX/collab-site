<div ng-controller="pageModuleViewController" class="uk-padding-large uk-padding-remove-top">
	

	<div id="module-sidebar" uk-offcanvas='overflow: true; mode: push'>
	    <div class="uk-offcanvas-bar">

	    	<div ng-controller='modulePageListController'>
	    		<div ng-show='api.loading'><div uk-spinner></div></div>
						
		        <ul  ng-show='!api.loading' class="uk-nav uk-nav-default">
		            <li class="uk-nav-header">{{master.value.name}}</li>
		            <li class="uk-active"><a href="javascript:void(0)">Navigation</a></li>
		            <li><a href="/#/guild/{{guild_id}}" onclick="UIkit.offcanvas('#module-sidebar').hide();" >Home</a></li>
		            <li class="uk-nav-header">Pages</li>
					<li ng-hide='api.list.length'>Nothing here.</li>

		            <li  ng-repeat='i in api.list track by $index'  ><a href="javascript:void(0)" ng-click='access[i.mode](i[i.mode],$index)'>{{i[i.mode].title}}</a></li>
		            

		            <li class="uk-nav-divider"></li>
		            <li><a href="#">Item</a></li>
		        </ul>
		    </div>

	    </div>
	</div>
	

	<div class="uk-padding">
		<a href="javascript:void(0)" class="uk-icon-button uk-margin-small-right" uk-toggle="target: #module-sidebar" uk-icon="menu"></a>
	</div>
	
	<div class="uk-padding-large uk-padding-remove-top">
		


		<div style="min-height: 80Vh" class="uk-padding-large">
			<div ng-show='subpage.here("")' class="uk-text-center">
				<br><br>
				<h2 class="uk-text-muted">Loading</h2><div uk-spinner='ratio: 1.5'></div>
			</div>
			<div ng-show='subpage.here("article")'>
				<div ng-controller='articleViewController'>
					<div ng-show='api.loading' class="uk-text-center">
						<br><br>
						<h2 class="uk-text-muted">Loading</h2><div uk-spinner='ratio: 1.5'></div>
					</div>
					<article ng-hide='api.loading' class="uk-article" >
					    <h1 style="text-align: center;"  class="uk-article-title uk"><a class="uk-link-reset" href="javascript:void(0)">{{api.value.title}}</a></h1>

					    <p  style="text-align: center;" class="uk-article-meta">Written by  <a ng-repeat='u in api.value.authors track by $index' href="#/user/{{u._id}}" target="_blank" >{{ u.name + ' ' }}</a>. Posted in <a href="javascript:void(0)">{{api.value.module.name}}</a></p>

					    <div class="uk-padding">
					    	<div ng-bind-html='api.value.content'></div>
					    </div>
					</article>
				</div>
			</div>
			<div ng-show='subpage.here("challenge")'>
				<div ng-controller='challengeViewController'>
					<div ng-show='api.loading' class="uk-text-center">
						<br><br>
						<h2 class="uk-text-muted">Loading</h2><div uk-spinner='ratio: 1.5'></div>
					</div>
					<div  ng-hide='api.loading' >
						<article class="uk-article" >
						    <h1 style="text-align: center;"  class="uk-article-title uk"><a class="uk-link-reset" href="javascript:void(0)">{{api.value.title}}</a></h1>

						    <p  style="text-align: center;" class="uk-article-meta">Written by  <a ng-repeat='u in api.value.authors track by $index' href="#/user/{{u._id}}" target="_blank" >{{ u.name + ' ' }}</a>. Posted in <a href="javascript:void(0)">{{api.value.module.name}}</a></p>

						    <div class="uk-padding">
						    	<div ng-bind-html='api.value.content'></div>
						    </div>
						</article>
						<hr class="uk-divider-icon">

						<div ng-show='api.value.output_type==1'> 
							<div style="text-align: center;">
								<h5>CHALLENGE RESPONSE</h5>
								Your answer
								<div class="uk-child-width-1-5 uk-padding-small">
									<input type="text" class="uk-input" >
								</div>
								<button class="uk-button uk-button-primary">SUBMIT</button>
							</div>
						</div>

						<div ng-show='api.value.output_type==0' >
							<div style="text-align: left;" class="uk-padding">
								<b>SAMPLE TEST CASES</b>
								<br><br>


								<div ng-repeat='i in api.value.testcases track by $index' class="uk-width-2-3">
									<b>TESTCASE {{$index}}</b> 	
									<div class="uk-child-width-1-2" uk-grid>
										<div>
											<small style="text-align: center;">INPUT</small>
											<pre class="uk-background-muted" style="min-height: 20vh; padding: 15px; margin-top: 0px"><code>{{i.input}}</code></pre>
										</div>
										<div>
											<small style="text-align: center;">OUTPUT</small>
											<pre class="uk-background-muted" style="min-height: 20vh; padding: 15px; margin-top: 0px"><code>{{i.output}}</code></pre>
										</div>
									</div>
									<br>
								</div>
								<br>
								<b>EXPLANATION</b>

								<p>{{api.value.settings.testcase_description}}</p>

								<hr class="uk-divider-icon">

								<div>
									<b>CHALLENGE RESPONSE</b>

									<ul uk-tab>
									    <li class="uk-active"><a href="#">Submit</a></li>
									    <li><a href="#">Previous Submissions</a></li>
									</ul>

										<ul class="uk-switcher uk-margin">
										    <li>
										    	<div   ng-controller='submissionNewController'>
													<div>
														<nav style="margin-bottom: 0px" class="uk-navbar uk-navbar-container uk-margin">
														    <div class="uk-navbar-left">
														        <a class="uk-navbar-toggle" uk-navbar-toggle-icon href="#"></a>  
														        Language &nbsp;&nbsp;
														        <div class="uk-margin">
														            <select  class="uk-select" ng-change='change_language()' ng-options='item as language_codes[item] for item in parent.value.settings.languages ' ng-model='model.language' style="border-radius: 10px"></select>
														        </div>
														    </div>
														</nav>
														
														<div class="uk-padding-remove-top ">
															<div>
																<textarea class="uk-border-rounded" id='editor-challenge'></textarea>
															</div>
														</div>

														<div class="uk-text-right">
															<br>
															<button ng-click='run_code()' class="uk-button uk-button-default uk-border-rounded">Run Code</button>
															<button  class="uk-button uk-button-default uk-border-rounded"  style="background-color: #59b761; color: white" href="#modal-submission-confirm" uk-toggle>Submit</button>
															<div id="modal-submission-confirm" uk-modal>
															    <div class="uk-modal-dialog">
															        <button class="uk-modal-close-default" type="button" uk-close></button>
															        <div class="uk-modal-header">
															            <h2 class="uk-modal-title">Submit Solution</h2>
															        </div>
															        <div class="uk-modal-body">
															            <p>Are you sure you want to submit code?</p>
															            <div ng-show='api.error' class="uk-alert uk-alert-danger">{{api.error}}</div>
															            <div ng-show='api.loading'><div uk-spinner></div></div>
															        </div>
															        <div ng-hide='api.loading' class="uk-modal-footer uk-text-right">
															            <button class="uk-button uk-button-danger uk-modal-close" type="button">Cancel</button>
															            <button ng-click='api.submit()' style="background-color: #59b761; color: white" class="uk-button uk-button-primary" type="button">Submit</button>
															        </div>
															    </div>
															</div>

														</div>
													</div>

													<div>
														

														<div>
															<br><br>
															<pre ng-show='run.loading' class="uk-background-muted" style="min-height: 2vh; padding: 15px; margin-top: 0px"><code>Loading...</code></pre>
																	
															<ul ng-hide='run.loading'  class="uk-tab-bottom" uk-tab>
															    <li ng-repeat='i in run.results track by $index' ><a href="#">Testcase {{$index+1}}</a></li>
															</ul>
															<div ng-show='run.error' class="uk-alert uk-alert-danger">
																{{run.error}}
															</div>												

															<ul class="uk-switcher uk-margin">
																<li ng-repeat='i in run.results track by $index'>
																	<div>
																		<div class="uk-text-center">
																			<b>VERDICT  </b> <br>
																			<b><em class="uk-text-capitalize" ng-class='{ "uk-text-success" : i.status.id==3 }'>{{ i.status.description  }}</em></b>
																		</div>
																		<pre ng-show='i.stderr' class="uk-background-muted" style="min-height: 2vh; padding: 15px; margin-top: 0px"><code>{{i.stderr}}</code></pre>
																		<pre ng-show='i.compile_output' class="uk-background-muted" style="min-height: 2vh; padding: 15px; margin-top: 0px"><code>{{i.compile_output}}</code></pre>
																		<div ng-hide='i.status.id == 6 || i.status.id == 13'>
																			<div class="uk-child-width-1-3" uk-grid>
																				<div>
																					<small style="text-align: center;">INPUT</small>
																					<pre class="uk-background-muted" style="min-height: 20vh; padding: 15px; margin-top: 0px"><code>{{parent.value.testcases[$index].input}}</code></pre>
																				</div>
																				<div>
																					<small style="text-align: center;">OUTPUT</small>
																					<pre class="uk-background-muted" style="min-height: 20vh; padding: 15px; margin-top: 0px"><code>{{parent.value.testcases[$index].output}}</code></pre>
																				</div>
																				<div>
																					<small style="text-align: center;">YOUR OUTPUT</small>
																					<pre class="uk-background-muted" style="min-height: 20vh; padding: 15px; margin-top: 0px"><code>{{i.stdout}}</code></pre>
																				</div>
																			</div>
																			
																		</div>
																	</div>		
																</li>
															</ul>

															<!-- <div>
																<div class="uk-text-center">
																	<b>YOUR SCORE</b> <br>
																	<b><em>10</em>/50</b>
																</div>
																<ul class="uk-list uk-list-striped">
																	<li> <span class="uk-margin-small-right" uk-icon="check"></span> Test Case 1 &middot; 10 &middot; <em>TLE</em> </li>
																	<li> <span class="uk-margin-small-right" uk-icon="check"></span> Test Case 2 &middot; 10 &middot; <em>TLE</em> </li>
																	<li> <span class="uk-margin-small-right" uk-icon="check"></span> Test Case 3 &middot; 10 &middot; <em>TLE</em> </li>
																	<li> <span class="uk-margin-small-right" uk-icon="check"></span> Test Case 4 &middot; 10 &middot; <em>TLE</em> </li>
																	<li> <span class="uk-margin-small-right" uk-icon="check"></span> Test Case 5 &middot; 10 &middot; <em>TLE</em> </li>
																</ul>
															</div> -->
															
															

															<hr class="uk-divider-icon">
														</div>
													</div>
										    		
										    	</div>
										    </li>
										    <li>
												<div>
													<table class="uk-table uk-table-striped">
													    <thead>
													        <tr>
													            <th>#</th>
													            <th>SCORE</th>
													            <th>VERDICT</th>
													        </tr>
													    </thead>
													    <tbody>
													        <tr>
													            <td>1</td>
													            <td>10/40</td>
													            <td><em>Wrong Answer</em></td>
													        </tr>
													        <tr>
													            <td>2</td>
													            <td>20/40</td>
													            <td><em>Time Limit Exceeded</em></td>
													        </tr>
													        <tr>
													            <td>3</td>
													            <td>50/50</td>
													            <td><em>Wrong Answer</em></td>
													        </tr>
													    </tbody>
													</table>
												</div>
										    </li>
										</ul>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>	

			
		</div>



		<ul class="uk-pagination">
		    <li ng-show='page!=0'><a href="javascript:void(0)" ng-click='page_prev()'><span class="uk-margin-small-right" uk-pagination-previous></span> Previous</a></li>
		    <li ng-show='page!=pages.length-1' class="uk-margin-auto-left"><a href="javascript:void(0)" ng-click='page_next()'>Next <span class="uk-margin-small-left" uk-pagination-next></span></a></li>
		</ul>
	</div>
</div>